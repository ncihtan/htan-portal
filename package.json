    "react-portal": "^4.2.1",
    "react-select": "^3.1.0",
    "react-spinners": "^0.13.8",
    "react-truncate-markup": "^5.1.0",
    "remark-frontmatter": "^4.0.1",
    "roman-numerals": "^0.3.2",
    "semver": "^7.5.2",
    "styled-components": "^5.1.0",
